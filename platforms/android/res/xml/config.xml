<?xml version='1.0' encoding='utf-8'?>
<widget id="io.cordova.IonicWithAngularjs" version="1.0.0" xmlns="http://www.w3.org/ns/widgets" xmlns:cdv="http://cordova.apache.org/ns/1.0" xmlns:vs="http://schemas.microsoft.com/appx/2014/htmlapps">
    <preference name="loglevel" value="DEBUG" />
    <feature name="HockeyApp">
        <param name="android-package" value="com.zengularity.cordova.hockeyapp.HockeyApp" />
